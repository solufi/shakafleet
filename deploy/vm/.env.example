# Fleet Manager
ADMIN_EMAIL=admin@example.com
ADMIN_PASSWORD_HASH=$2b$12$REPLACE_ME_WITH_BCRYPT_HASH
AUTH_SECRET=REPLACE_ME_WITH_LONG_RANDOM_SECRET

# Postgres
POSTGRES_DB=fleet
POSTGRES_USER=fleet
POSTGRES_PASSWORD=REPLACE_ME_WITH_STRONG_PASSWORD
DATABASE_URL=postgresql://fleet:REPLACE_ME_WITH_STRONG_PASSWORD@postgres:5432/fleet

# MinIO (S3-compatible)
MINIO_ROOT_USER=REPLACE_ME
MINIO_ROOT_PASSWORD=REPLACE_ME

# Optional image tag override
FLEET_TAG=latest
